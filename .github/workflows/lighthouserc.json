{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "settings": {
        "emulatedFormFactor": "desktop"
      },
      "puppeteerScript" : ".github/workflows/puppeteer-script.js"
    },
    "assert": {
      "assertions": {
        "accesskeys": ["error", {}],
